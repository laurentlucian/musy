{
  "name": "musy",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "release": "wrangler deploy",
    "typecheck": "react-router typegen && tsc",
    "format": "biome format --write ./app",
    "lint": "biome lint",
    "lint:fix": "biome lint --write --unsafe",
    "db:migrate": "wrangler d1 migrations apply -c ./wrangler.jsonc --local musy",
    "typegen": "wrangler types -c ./wrangler.jsonc --env-interface Env",
    "env": "dotenv -e .env"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "@base-ui/react": "1.0.0",
    "@better-fetch/fetch": "1.1.21",
    "@radix-ui/react-slot": "1.2.3",
    "@tailwindcss/vite": "4.1.18",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "dotenv": "17.2.3",
    "drizzle-kit": "0.31.5",
    "drizzle-orm": "0.45.1",
    "lucide-react": "0.546.0",
    "motion": "12.23.26",
    "nanoid": "^5.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router": "7.11.0",
    "remix-auth": "4.2.0",
    "remix-auth-oauth2": "3.4.1",
    "remix-utils": "9.0.0",
    "sonner": "2.0.7",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.18",
    "tailwindcss-animate": "1.0.7",
    "valibot": "1.2.0",
    "xstate": "5.25.0",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@cloudflare/vite-plugin": "1.17.1",
    "@react-router/dev": "7.11.0",
    "@react-router/fs-routes": "7.11.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vite-tsconfig-paths": "6.0.3"
  },
  "overrides": {
    "use-sync-external-store": "1.5.0"
  }
}
