    "eslint-plugin-turbo": "^0.0.7",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "generate-changelog": "^1.8.0",
    "husky": "^7.0.1",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.8",
    "prisma": "^3.15.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "zod": "3.19.1"
  },
  "engines": {
    "node": ">=14"
  }
}
