{
  "name": "musy",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "start": "react-router-serve build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "release": "bun run build && wrangler deploy",
    "format": "biome format --write ./app",
    "lint": "biome lint",
    "lint:fix": "biome lint --write --unsafe",
    "prisma:generate": "prisma generate --schema=prisma/schema",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:create": "prisma migrate dev --create-only",
    "db:migrate": "wrangler d1 migrations apply -c ./wrangler.jsonc --local musy",
    "typegen": "wrangler types -c ./wrangler.jsonc --env-interface ServerEnv"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "@ai-sdk/groq": "1.2.9",
    "@ai-sdk/openai": "1.3.21",
    "@ai-sdk/xai": "1.2.16",
    "@mjackson/headers": "0.11.1",
    "@prisma/adapter-d1": "6.16.2",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-context-menu": "2.2.16",
    "@radix-ui/react-select": "2.2.4",
    "@radix-ui/react-slot": "1.2.3",
    "@react-router/serve": "7.9.2",
    "@remix-run/node-fetch-server": "0.9.0",
    "@t3-oss/env-core": "0.13.8",
    "@tailwindcss/vite": "4.1.13",
    "@xstate/react": "6.0.0",
    "ai": "4.3.15",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "compression": "1.8.0",
    "cross-env": "7.0.3",
    "date-fns": "4.1.0",
    "discord.js": "14.16.3",
    "dotenv": "16.5.0",
    "intl-parse-accept-language": "1.0.0",
    "is-ip": "5.0.1",
    "isbot": "5.1.27",
    "lucide-react": "0.508.0",
    "morgan": "1.10.0",
    "motion": "12.10.4",
    "prisma": "6.16.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router": "7.9.2",
    "remix-auth": "4.2.0",
    "remix-auth-oauth2": "3.4.1",
    "remix-utils": "8.5.0",
    "sonner": "2.0.3",
    "spotified": "1.1.5",
    "tailwind-merge": "3.2.0",
    "tailwindcss-animate": "1.0.7",
    "tiny-invariant": "1.3.3",
    "valibot": "1.0.0",
    "xstate": "5.22.0",
    "ytmusic-api": "5.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@cloudflare/vite-plugin": "1.13.5",
    "@react-router/dev": "7.9.2",
    "@react-router/fs-routes": "7.9.2",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-rsc": "0.4.31",
    "typescript": "5.9.2",
    "vite": "7.1.7",
    "vite-tsconfig-paths": "5.1.4",
    "wrangler": "4.40.0"
  },
  "overrides": {
    "use-sync-external-store": "1.5.0"
  },
  "patchedDependencies": {
    "spotified@1.1.5": "patches/spotified@1.1.5.patch"
  }
}
