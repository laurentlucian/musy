{
  "name": "musy",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "vite preview",
    "release": "bun run build && wrangler deploy",
    "format": "biome format --write ./app",
    "lint": "biome lint",
    "lint:fix": "biome lint --write --unsafe",
    "typecheck": "react-router typegen && tsc -b",
    "prisma:generate": "prisma generate --schema=prisma/schema",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:create": "prisma migrate dev --create-only",
    "db:migrate": "wrangler d1 migrations apply -c ./wrangler.jsonc --local musy",
    "typegen": "wrangler types -c ./wrangler.jsonc --env-interface ServerEnv"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "@ai-sdk/groq": "^1.2.9",
    "@ai-sdk/openai": "^1.3.21",
    "@ai-sdk/xai": "^1.2.16",
    "@mjackson/headers": "^0.10.0",
    "@prisma/adapter-d1": "^6.16.2",
    "@prisma/client": "6.16.2",
    "@radix-ui/react-context-menu": "^2.2.14",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@react-router/express": "7.6.0",
    "@react-router/node": "7.6.0",
    "@t3-oss/env-core": "^0.13.4",
    "@tailwindcss/vite": "^4.1.13",
    "@xstate/react": "^5.0.3",
    "ai": "^4.3.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.0",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.5.0",
    "intl-parse-accept-language": "^1.0.0",
    "is-ip": "^5.0.1",
    "isbot": "^5.1.27",
    "lucide-react": "^0.508.0",
    "morgan": "^1.10.0",
    "motion": "^12.10.4",
    "prisma": "6.16.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "7.9.1",
    "remix-auth": "^4.2.0",
    "remix-auth-oauth2": "^3.4.0",
    "remix-utils": "^8.5.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "tiny-invariant": "^1.3.3",
    "valibot": "^1.0.0",
    "xstate": "^5.19.2",
    "ytmusic-api": "^5.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@cloudflare/vite-plugin": "^1.13.3",
    "@react-router/dev": "7.6.0",
    "@types/compression": "^1.7.5",
    "@types/debug": "^4.1.12",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "bun-types": "^1.2.12",
    "lint-staged": "^15.5.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.38.0"
  }
}
